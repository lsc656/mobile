/* pages/pins/pins.wxss */
.header{
  display:flex;
  height:100rpx;
  padding:10rpx;
}
.header>image{
  height:100rpx;
  width:100rpx;
}
.header>view{
  margin:0 10rpx 0 20rpx;
  display:flex;
  flex-direction: column;
  justify-content: space-between;
}
.header>view>view:nth-child(2){
  color:#999999;
  font-size:24rpx;
}
.header>button{
  padding:10rpx 20rpx;
}